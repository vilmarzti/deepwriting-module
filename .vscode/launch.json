{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Node flatten",
            "type": "pwa-node",
            "request": "launch",
            "program": "${workspaceFolder}/svg_util/flatten.ts",
            "preLaunchTask": "tsc: build - svg_util/tsconfig.json",
            "outFiles": ["${workspaceFolder}/svg_util/build/*.js"],
            "args": ["data/abc_svg/a.svg"]
        },
        {
            "name": "Python: Simple Server",
            "type": "python",
            "request": "launch",
            "module": "keras.simple.server" 
        },
        {
            "name": "Python: Seq2Seq Server",
            "type": "python",
            "request": "launch",
            //"program": "${file}",
            "module": "keras.seq2seq.server",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Deewpriting Server",
            "type": "python",
            "request": "launch",
            "program": "deepwriting/server.py"
        }
    ]
}